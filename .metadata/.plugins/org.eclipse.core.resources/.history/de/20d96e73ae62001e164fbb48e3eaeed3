package Patterns;
import InputOutput.pattern_IO;

public class RectangleOrSquare {
	
	public void print()
	{
		pattern_IO.ioRowCol();
		int row=pattern_IO.row;
		int col=pattern_IO.col;
		for(int i=1;i<=row;i++)
		{
			for(int j=1;j<=col;j++)
			{
				System.out.print("* ");
			}
			System.out.println();
		}
	}
}
